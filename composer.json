{
	"name": "litstack/rehearsal",
	"type": "composer-plugin",
	"license": "MIT",
	"authors": [
		{
			"name": "cbl",
			"email": "lennart.carbe@gmail.com"
		}
	],
	"require": {
		"composer-plugin-api": "^1.1|^2.0",
		"orchestra/testbench": "^5.3",
		"aw-studio/fjord": "^2.4"
	},
	"autoload": {
		"psr-4": {
			"FjordApp\\": "../../orchestra/testbench-core/laravel/fjord/app",
			"Fjuse\\Testbench\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"FjordApp\\": "vendor/orchestra/testbench-core/laravel/fjord/app"
		}
	},
	"extra": {
		"class": "Fjuse\\Testbench\\InstallerPlugin"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.3"
	},
	"scripts": {
		"post-autoload-dump": [
			"@php bin/install"
		]
	}
}
